html
  background #fff
body
  --main #4786D6
  --text #333
  --text2 #444
  --darker #000
  --btn-bg #333
  --btn-text #fff
  --bg #fff
  --bg2 #efefef
  --border #ddd
  --border2 #888
  --border-type dotted
  --member #e7b836
  text-rendering optimizeLegibility
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  font-family 'Anyway Type', 'Myriad Pro', 'PingFang SC', 'Microsoft YaHei', sans-serif
  text-align left
  color var(--text)
  line-height 1
  background var(--bg)
  // margin 0.8rem 0.8rem 0
*
  margin 0
  padding 0
  border 0
  vertical-align baseline
  outline 0
  box-sizing border-box
  -webkit-tap-highlight-color transparent
a
  color inherit
  cursor pointer
  transition color 0.15s cubic-bezier(0, 0.3, 0.8, 1), background-color 0.15s cubic-bezier(0, 0.3, 0.8, 1)
img
  image-rendering -webkit-optimize-contrast
.container
  display flex
  flex-direction row
  background var(--bg)
  transition background-color 0.12s ease-out

@import '_partial/header'